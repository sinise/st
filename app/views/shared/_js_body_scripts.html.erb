<script>
	UserVoice=window.UserVoice||[];
	(function(){
		var uv=document.createElement('script');
		uv.type='text/javascript';
		uv.async=true;
		uv.src='//widget.uservoice.com/ofln8Hio3GE0icf5FOSpNA.js';
		var s=document.getElementsByTagName('script')[0];
		s.parentNode.insertBefore(uv,s)
	})();

	UserVoice.push(['set', {
		accent_color: '#1abc9c',
		contact_enabled: true,
		locale: 'da',
		post_idea_enabled: false,			
		screenshot_enabled: true,
		smartvote_enabled: false,
		trigger_background_color: '#1abc9c',
		trigger_color: 'white'
	}]);

	<% if current_user %>
		UserVoice.push(['identify', {
			id: '<%= current_user.id %>',
			email: '<%= current_user.email %>'
			<% if current_user.user_detail %>
				<%=raw ",name: '" + current_user.user_detail.first_name + " " + current_user.user_detail.last_name + "'" %>
			<% end %>
		}]);
	<% end %>
	
	UserVoice.push(['addTrigger', { mode: 'contact', trigger_position: 'bottom-right' }]);
</script>

