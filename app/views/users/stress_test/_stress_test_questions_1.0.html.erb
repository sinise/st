            <td class="text-center">
              <%= f.radio_button :q6, i, :checked => @stress_test.results['q6'] == i.to_s %>
            </td>
        <% end %>
    </tr>

    <tr class="row-last">
        <td class="cell-question">...opleve, at du ikke bevægede dig fremad?</td>
        <% 1.upto(5) do |i| %>
            <td class="text-center">
              <%= f.radio_button :q7, i, :checked => @stress_test.results['q7'] == i.to_s %>
