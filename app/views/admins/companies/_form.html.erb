<div class="row">
	<div class="col-md-6">

		<%= form_for(@company) do |f| %>
			<% if @company.errors.any? %>
				<div class="alert alert-danger">
					<ul>
						<% @company.errors.full_messages.each do |msg| %>
							<li><%= msg %></li>
						<% end %>
					</ul>
				</div>
			<% end %>

		  <div class="form-group">
		    <%= f.label :name, 'Navn' %>
		    <%= f.text_field :name, :class => "form-control", :autofocus => true %>
		  </div>

		  <div class="form-group">
		    <%= f.label :code, 'Kode' %>
		    <%= f.text_field :code, :class => "form-control" %>
		  </div>

		  <div class="form-group">
		    <%= f.label :url, 'Url' %>
		    <%= f.text_field :url, :class => "form-control" %>
		  </div>

		  <div class="form-group">
		    <%= f.label :phone, 'Telefon' %>
		    <%= f.text_field :phone, :class => "form-control" %>
		  </div>

		  <div class="form-group">
		    <%= f.label :email %>
		    <%= f.text_field :email, :class => "form-control" %>
		  </div>

		  <div class="form-group">
		    <%= f.label :session_hourly_rate, 'Hourly rate' %>
		    <%= f.text_field :session_hourly_rate, :class => "form-control" %>
		  </div>

		  <div class="form-group">
		    <%= f.label :currency, 'Currency' %>
		    <%= f.text_field :currency, :class => "form-control" %>
		  </div>

			<div class="form-actions">
				<%= f.submit 'Gem', :class => "btn btn-lg btn-primary" %>
				<%= link_to 'Tilbage', admins_companies_path, :class => "btn btn-lnk pull-left" %>
			</div>

		<% end %>

	</div>
</div>